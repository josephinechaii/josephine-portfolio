      <?php include("includes/footer.html"); ?>
      <script>
      $(document).ready(function(){
        // Add smooth scrolling to all links
        $("nav a").on('click', function(event) {
          if (this.hash !== "") {
            event.preventDefault();
            var hash = this.hash;
            $('html, body').animate({
              scrollTop: $(hash).offset().top
            }, 800, function(){
              window.location.hash = hash;
            });
          }
        });
      });
      </script>
      <script src="js/typeit.js"></script>
      <script>
      $('.type-it').typeIt({
        autoStart: false,
        deleteSpeed: 100
      })
      .tiSettings({speed: 50})
      .tiType('Hey')
      .tiPause(500)
      .tiDelete(1)
      .tiType('llo!')
      .tiPause(1000)
      .tiDelete(6)
      .tiType('My name is Erie :)')
      .tiPause(500)
      .tiDelete(4)
      .tiType('ca :)')
      .tiBreak()
      .tiSettings({speed: 200})
      .tiBreak()
      .tiType('Who am I?')
      .tiPause(500)
      .tiEmpty()
      .tiSettings({speed: 50})
      .tiType('I am a ... ')
      .tiSettings({speed: 150})
      .tiType('scientist')
      .tiPause(500)
      .tiDelete(7)
      .tiType('artist')
      .tiPause(500)
      .tiDelete(9)
      .tiType('writer')
      .tiPause(500)
      .tiDelete(10)
      .tiType('photographer')
      .tiPause(500)
      .tiDelete(12)
      .tiType('coder')
      .tiDelete(8)
      .tiDelete(19)
      .tiType('Check out my website');
      </script>
